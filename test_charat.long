namespace Test

use System.Console

class TestCharAt {
    public static function main() {
        Console::writeLine("测试 charAt 方法...")
        
        str := "hello"
        Console::writeLine("字符串: " + str)
        
        ch := str.charAt(0)
        Console::writeLine("charAt(0): " + ch)
        
        ch2 := str.charAt(1)
        Console::writeLine("charAt(1): " + ch2)
        
        Console::writeLine("测试完成!")
    }
}
